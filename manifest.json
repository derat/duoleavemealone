{
  "name": "duoleavemealone for Duolingo",
  "short_name": "duoleavemealone",
  "version": "0.6",
  "description": "Makes Duolingo more user-friendly",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.duolingo.com/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
